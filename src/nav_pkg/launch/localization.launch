<launch> 

	<!-- Load URDF -->
  	<param name="robot_description" textfile="$(find cartographer_ros)/urdf/saxx.urdf" />

  	<!-- Robot State Publisher -->
 	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" output="screen" />
	<node name="map_server" pkg="map_server" type="map_server" args="$(find nav_pkg)/maps/map.yaml" output="screen"/>

	<include file="$(find rplidar_ros)/launch/view_rplidar_a1.launch"/>
  	<include file="$(find ackermann_encoder_odometry)/launch/encoder_odometry.launch"/>

	<node name="amcl" pkg="amcl" type="amcl" output="screen">
	<param name="use_map_topic" value="true"/>
	<param name="min_particles" value="500"/>
	<param name="max_particles" value="2000"/>
	<param name="odom_frame_id" value="odom"/>
	<param name="base_frame_id" value="base_link"/>
	<param name="global_frame_id" value="map"/>
	</node>

</launch>
